//Setting inicial colors and window size
background(0);
fill(0);
stroke(255, 255, 0);
size(500, 500);

//Drawing smilies

for (int i = 0; i < 10; i++) {
  for (int e = 0; e < 10; e++) {
    //Happy
    ellipse(25+(100*i),30+(50*e), 50,50);
    line(20+(100*i),45 +(50*e), 30+(100*i),45 +(50*e));
    line(17+(100*i),42 +(50*e), 20+(100*i),45 +(50*e));
    line(30+(100*i),45 +(50*e), 33+(100*i),42 +(50*e));
    ellipse(25+(100*i),30 +(50*e), 5,5);
    ellipse(14+(100*i),17 +(50*e), 5,5);
    ellipse(36+(100*i),17 +(50*e), 5,5);
    
    //Sad
    ellipse(50+25+(100*i),30+(50*e), 50,50);
    line(50+20+(100*i),45 +(50*e), 50+30+(100*i),45 +(50*e));
    line(50+17+(100*i),48 +(50*e), 50+20+(100*i),45 +(50*e));
    line(50+30+(100*i),45 +(50*e), 50+33+(100*i),48 +(50*e));
    ellipse(50+25+(100*i),30 +(50*e), 5,5);
    ellipse(50+14+(100*i),17 +(50*e), 5,5);
    ellipse(50+36+(100*i),17 +(50*e), 5,5);
  }
}
